package org.KH.app2;
// 중첩 For 문을 활용한 별 찍기
public class StarPrint {

	public static void main(String[] args) {
		/*
		 
		 ******
		 ******
		 ******
		 ******
		 
		 */

		for(int i=0 ; i<4 ; i++) {			//	0 ~ 3 까지 4번 실행됨
			for(int j=0 ; j<6 ; j++ ) {		//	0 ~ 5 까지 6번 실행됨
				System.out.println("*");
			}
		}
		
	}

}
