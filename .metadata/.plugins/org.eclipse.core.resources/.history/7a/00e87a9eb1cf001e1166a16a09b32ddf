package org.KH.java;

public class Variable7 {

	public static void main(String[] args) {
		String name = "계성우";
		int age = 27;
		float height = 170.00123f;
		boolean pass = true;
		double weight = 62.03;

		
		System.out.println("name :"+name.getClass().getName());
		System.out.println("age :"+age.getClass().getName());
	}

}
