package org.KH.app2;
// 중첩 For 문을 활용한 별 찍기
public class StarPrint {

	public static void main(String[] args) {
		/*
		 
		 ******
		 ******
		 ******
		 ******
		 
		 ~ 을 출력해보자
		 
		 */

//		for(int i=0 ; i<4 ; i++) {			// 0 ~ 3 까지 4번 실행됨
//			for(int j=0 ; j<6 ; j++ ) {		// i 가 실행 될 때마다 0 ~ 5 까지 6번 실행되어 총 24번 실행됨
//				System.out.print("*");	// * 24개 찍힘
//			}
//			System.out.println();
//		}
		
		for(int a=0 ; a<1; a++) {
			for(int b=0 ; b<2 ; b++) {
				for(int c=0 ; c<3 ; c++)
					for(int d=0 ; d<3 ; d++)
						for(int e=0 ; e<4 ; d++)
							for(int f=0 ; f<5 ; f++) {
								System.out.print("*");
							}
			}
			System.out.println();
		}
			
		
	}

}
