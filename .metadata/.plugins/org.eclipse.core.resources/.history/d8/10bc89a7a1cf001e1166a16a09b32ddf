package org.KH.java;

public class Variable4 {

	public static void main(String[] args) {
		String name = "\'계성우\'";
		short age = 27;
		float height = 170;
		double weight = 62;
		char init = 'k';
		
		/* 이스케이프 문자
		 \n : 줄바꿈
		 \t : 탭
		 \\ : \를 출력
		 \" : " 를 출력
		 \' : ' 를 출력
		 */

		 
		System.out.println("이름 : \""+name+"\"");
	
	}

}
