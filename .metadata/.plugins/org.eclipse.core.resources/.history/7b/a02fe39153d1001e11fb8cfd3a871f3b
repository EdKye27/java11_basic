package org.KH.app2;
// 중첩 For 문을 활용한 별 찍기
public class StarPrint {

	public static void main(String[] args) {
		/*
		 
		 ******
		 ******
		 ******
		 ******
		 
		 */

		for(int i=0 ; i<4 ; i++) {			// 0 ~ 3 까지 4번 실행됨
			for(int j=0 ; j<6 ; j++ ) {		// i 가 실행 될 때마다 0 ~ 5 까지 6번 실행되어 총 24번 실행됨
				System.out.print("*");	// 세로로 * 24개 찍힘
			}
			System.out.println();
		}
		
	}

}
