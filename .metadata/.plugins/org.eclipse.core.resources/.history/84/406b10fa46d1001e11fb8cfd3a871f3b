package org.KH.app2;
//do ~ while : 조건이 만족하지 않더라도 1회 이상 실행을 보장하는 문장
public class Loop5 {

	public static void main(String[] args) {
		int a = 0;
		while(a>10) {
			a++;
			System.out.println("a="+a);
		}
		// → 이렇게 되면 값이 안나오겠지 값이 성립이 안되니까 do ~ while ~ 사용해보자
		do {
			System.out.println("a="+a);
		} while(a>10);

	}

}
